<h1>AdminSessions#new</h1>

<%= form_for :admin_session, :url=> admin_admin_sessions_path do |form| %>
<%= form.error_messages %>
<ol class="formList">
  <li>
    <%= form.label :login, "Login" %>
    <%= form.text_field :login %>
  </li>
  <li>
    <%= form.label :password, "Password" %>
    <%= form.password_field :password %>
  </li>
  <li>
    <%= form.submit "Submit" %>
  </li>
</ol>
<% end %>