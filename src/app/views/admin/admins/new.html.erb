<%= form_for :admin, :url=>admin_admins_path, :html=>{:method=>'post'} do |form| %>
<div class="pgTitle">
  <h2 class="f_l">新增管理员</h2>
  <div class="completeBtn newBtn">
    <%=  form.submit "保存"%>
    <%= button_to_function "取消", "location.href='#{admin_admins_path}'"%>
  </div>
  <hr class="cb" />
</div>
<div><%= error_messages_for @admin %></div>    
<div class="commonBox">
  <%= render :partial => 'form', :locals => {:form => form }%>
</div>

<% end%>
